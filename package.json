{
  "name": "@hhenrichsen/motion-canvas-nord",
  "version": "1.1.1",
  "description": "Nord Theme for Motion Canvas",
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "eslint": "eslint \"**/*.ts?(x)\"",
    "eslint:fix": "eslint --fix \"**/*.ts?(x)\"",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "npm run eslint && npm run prettier"
  },
  "unpkg": "lib/bundle.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hhenrichsen/motion-canvas-nord.git"
  },
  "keywords": [
    "motion-canvas"
  ],
  "author": "Hunter Henrichsen <hunter.henrichsen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hhenrichsen/motion-canvas-nord/issues"
  },
  "homepage": "https://github.com/hhenrichsen/motion-canvas-nord#readme",
  "peerDependencies": {
    "@motion-canvas/core": "^3.9.0",
    "code-fns": "^0.8.2"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^9.21.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.0",
    "rollup": "^4.0.2",
    "rollup-plugin-dts": "^6.0.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,scss}": "prettier --write"
  },
  "dependencies": {
    "@motion-canvas/2d": "^3.9.0",
    "@motion-canvas/ui": "^3.9.0",
    "@motion-canvas/vite-plugin": "^3.9.0",
    "vite": "^5.0.10"
  }
}
